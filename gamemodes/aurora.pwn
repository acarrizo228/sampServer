				}
				if(!IsAArm(playerid)) {
					new mats = floatround(FI[GetTeamID(playerid)][fMats]/500);//ящиков на складе банды
					new mats_2 = mats+(VG[vehicleid][vgAmount][0]*5000/500);//сколько будет всего ящиков
