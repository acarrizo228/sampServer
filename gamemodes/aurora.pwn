	TI[playerid][tRaceID] = TI[TI[playerid][tRaceID]][tRaceID] = INVALID_PLAYER_ID;
}
stock save_fracveh(slot) {
	A_DestroyVehicle(FracVehicle[slot][fvehCar]);
