	new string[128];
	format(string, sizeof(string), "[A] %s[%d] установил %d сытости игроку %s[%d]",player_name[playerid],playerid,hp,player_name[giveplayerid],giveplayerid);
	AdmMSG(0xAFAFAFAA, string,1);
	return true;
}
