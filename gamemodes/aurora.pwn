GetNearestATM(playerid) {
	new atmID = -1;
	for (new i = 1; i < MAX_ATM; i ++) {
		if (ATMData[i][atm_Taken] && IsPlayerInRangeOfPoint(playerid, 2.0, ATMData[i][ATM_Pos][0], ATMData[i][ATM_Pos][1], ATMData[i][ATM_Pos][2])) {
