		}
		if(PI[playerid][pSearch] >= 1) SetPlayerWantedLevel(playerid,PI[playerid][pSearch]);
	}
	switch(PI[playerid][pSettings][5]) {
	    case 0: SetPlayerFightingStyle(playerid, FIGHT_STYLE_NORMAL);
