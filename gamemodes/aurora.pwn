	}
	else if(areaid == ShootingArea[playerid]) { //тир зона
		new mapshooting, maxbox;
		switch(GetPVarInt(playerid,"shootingpl")) {
			case 67: mapshooting = 0, maxbox = 4; 
			case 78: mapshooting = 11, maxbox = 8;  // lva
