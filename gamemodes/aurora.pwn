stock NonSym(const str[], max = 0, min = 0)  {
	static BadNameChars[][2] = {"/","\n","`","~","%","^","&","[","]","{","}","|","'"};
	for(new i = 0; i < sizeof( BadNameChars ); i++) {
		if(strfind( str, BadNameChars[i], true ) != -1 || strlen( str ) > max || strlen( str ) < min) return 1;
	}
	return 0;
}
