				format(string,sizeof(string), "Стрела за бизнес %s закончилась ничьей. Бизнес оставлен",gBusiness[MZInfo[bBiz]][bizzName]);
				SendFamilyMessage(MZInfo[bNapad], 0xFF0000FF, string);
				UpdateBusinessText(MZInfo[bBiz]);
