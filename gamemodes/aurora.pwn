			PlayerTextDrawHide(playerid,work_td[playerid][i]);
		}
	}
	if(GetTeamID(playerid) == 0) start_work[playerid] = 0;
	if(IsAGang(playerid) && IsAMafia(playerid)) start_work[playerid] = 1;
